{
  "workspace.ignoredFiletypes": [
    "markdown",
    "log",
    "txt",
    "help"
  ],
  "coc.preferences.rootPatterns": [
    ".git",
    ".vim",
    ".hg",
    ".projections.json"
  ],
  "snippets.userSnippetsDirectory": "~/.config/nvim/UltiSnips/",
  "vetur.ignoreProjectWarning": true,
  "coc.preferences.extensionUpdateCheck": "daily",
  "coc.preferences.colorSupport": true,
  "coc.preferences.formatOnType": true,
  "suggest.floatEnable": true,
  "suggest.maxCompleteItemCount": 20,
  "json.enable":true,
  "json.format.enable":true,
  "diagnostic.enable": true,
  "list.height": 30,
  "list.insertMappings": {
    "<A-x>": "call:OpenWithSystemApp",
    "<A-r>": "call:OpenWithFinder"
  },
  "picgo.dataPath": "/Users/Real/.picgo/data.json",
  "picgo.configPath": "/Users/Real/.picgo/config.json",
  "Lua.diagnostics.globals": ["vim", "setfenv"],
  "Lua.runtime.version": "Lua 5.1",
  "javascript.suggestionActions.enabled": false,
  "coc-helper.projects": {
    "DocBase": "~/Documents/DocBase/",
    "Downloads": "~/Downloads",
    "Dotfile": "~/code/whyreal/dotfiles/",
    "Vimconfig": "~/code/whyreal/dotfiles/vim/"
  },
  "python.jediEnabled": false
}
