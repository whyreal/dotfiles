{
	"title": "home line ctrl",
	"rules":[
		{
			"description": "Change i to Control when used as modifier, i when used alone",
			"manipulators": [
				{
					"from": {
						"key_code": "i",
						"modifiers": {
							"optional": [
								"any"
							]
						}
					},
					"to": [
						{
							"key_code": "left_control",
							"lazy": true
						}
					],
					"to_if_alone": [
						{
							"key_code": "i"
						}
					],
					"type": "basic"
				}
			]
		},
		{
			"description": "Change e to Control when used as modifier, e when used alone",
			"manipulators": [
				{
					"from": {
						"key_code": "e",
						"modifiers": {
							"optional": [
								"any"
							]
						}
					},
					"to": [
						{
							"key_code": "left_control",
							"lazy": true
						}
					],
					"to_if_alone": [
						{
							"key_code": "e"
						}
					],
					"type": "basic"
				}
			]
		}
	]
}
