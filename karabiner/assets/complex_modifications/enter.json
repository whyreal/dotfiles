{
	"title": "Enter",
	"rules":[
		{
			"description": "Change Return to Control when used as modifier, Return when used alone",
			"manipulators": [
				{
					"from": {
						"key_code": "return_or_enter",
						"modifiers": {
							"optional": [
								"any"
							]
						}
					},
					"to": [
						{
							"key_code": "right_control",
							"lazy": true
						}
					],
					"to_if_alone": [
						{
							"key_code": "return_or_enter"
						}
					],
					"type": "basic"
				}
			]
		}
	]
}
